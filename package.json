{
  "name": "tax_calculator",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://vincentypy.github.io/bowtax",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "coverage": "nyc -r text -r cobertura yarn test",
    "format:check": "npx prettier --list-different 'src/**/*.{js,jsx,ts,tsx,css,scss,json}'",
    "format:fix": "npx prettier --write --list-different 'src/**/*.{js,jsx,ts,tsx,css,scss,json}'",
    "lint:script": "eslint 'src/**/*.{ts,tsx,js,jsx}'",
    "lint:style": "stylelint --max-warnings 0 'src/**/*.{css,scss}'",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "ci": "yarn format:check && yarn lint:script && yarn lint:style && yarn typecheck && yarn coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@craco/craco": "^6.3.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.13.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "autoprefixer": "^9",
    "i18next": "^22.5.1",
    "postcss": "^8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-scripts": "5.0.1",
    "react-tooltip": "^5.21.1",
    "tailwindcss": "^3",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/node-logger": "^6.5.13",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "eslint-plugin-tsdoc": "0.2.4",
    "gh-pages": "^6.3.0",
    "nyc": "15.1.0",
    "prettier": "2.8.8",
    "prop-types": "^15.8.1",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-no-unsupported-browser-features": "4.0.0",
    "webpack": "^5.74.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
